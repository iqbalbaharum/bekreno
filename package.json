{
  "name": "krenolms",
  "version": "1.0.0",
  "description": "Iqbal based NodeJS framework",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t krenolms .",
    "docker:run": "docker run -p 3000:3000 -d krenolms",
    "migrate": "node -r dotenv/config ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r dotenv/config -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^6.0.0",
    "@loopback/authorization": "^0.6.5",
    "@loopback/boot": "^2.5.0",
    "@loopback/core": "^2.9.4",
    "@loopback/repository": "^3.1.0",
    "@loopback/rest": "^6.1.0",
    "@loopback/rest-explorer": "^2.2.9",
    "@loopback/service-proxy": "^2.3.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.5",
    "@types/nodemailer": "^6.4.0",
    "@types/xml2js": "^0.4.5",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "jsonwebtoken": "^8.5.1",
    "loopback-component-storage": "^3.7.0",
    "loopback-connector-mysql": "^5.4.4",
    "loopback-connector-onesignal": "^1.0.14",
    "loopback-connector-rest": "^3.7.0",
    "moment": "^2.27.0",
    "nodemailer": "^6.4.11",
    "onesignal-node": "^3.1.5",
    "otplib": "^12.0.1",
    "tslib": "^2.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.1",
    "source-map-support": "^0.5.19",
    "@loopback/testlab": "^3.2.3",
    "@types/node": "^10.17.28",
    "@loopback/eslint-config": "^9.0.1",
    "eslint": "^7.7.0",
    "typescript": "~3.9.7"
  }
}
